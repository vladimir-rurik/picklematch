{
    "scripts": {
      "watch":   "dotenv -e .env -- shadow-cljs watch app",
      "release": "dotenv -e .env -- shadow-cljs release app",
      "build":   "shadow-cljs release app",
      "deploy":  "dotenv -e .env -- shadow-cljs release app && firebase deploy --only hosting"
    },
    "devDependencies": {
        "dotenv-cli": "^8.0.0",
        "shadow-cljs": "^2.28.23"
    },
    "dependencies": {
        "firebase": "^9.19.1",
        "react": "^18.2.0",
        "react-dom": "^18.2.0"
    }
}
