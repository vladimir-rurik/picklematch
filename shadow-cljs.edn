{:source-paths
 ["src"]

 :dependencies
 [[org.clojure/clojure "1.11.1"]
  [org.clojure/clojurescript "1.10.866"]
  [reagent "1.1.1"]
  [re-frame "1.2.0"]]

 :builds
 {:app
  {:target     :browser
   :output-dir "resources/public/js"
   :asset-path "/js"
   :modules    {:main {:init-fn picklematch.core/init}}
   :devtools   {:http-root "resources/public"
                :http-port 3000}}}}

:test
{:target :node-test
 :output-to "target/test/test.js"
 :autorun true}